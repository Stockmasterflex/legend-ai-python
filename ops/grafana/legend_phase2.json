      "datasource": "Prometheus",
      "fieldConfig": {
        "defaults": {"unit": "percent"},
        "overrides": []
      },
      "gridPos": {"h": 6, "w": 12, "x": 0, "y": 14},
      "id": 5,
      "targets": [
        {
          "expr": "(increase(analyze_errors_total[1d]) + increase(scan_errors_total[1d])) / (increase(analyze_request_duration_seconds_count[1d]) + increase(scan_request_duration_seconds_count[1d])) * 100",
