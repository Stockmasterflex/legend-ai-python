# Legend AI Environment Variables Template
# Copy this file to .env and fill in your actual values
# NEVER commit .env to version control

# Database
DATABASE_URL=postgresql://user:password@host:5432/legend_ai

# Redis
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_PASSWORD=

# Market Data API Keys
TWELVEDATA_API_KEY=your_twelvedata_key_here
FINNHUB_API_KEY=your_finnhub_key_here
ALPHA_VANTAGE_KEY=your_alphavantage_key_here

# Charting
CHART_IMG_API_KEY=your_chartimg_key_here

# Telegram Bot (Optional)
TELEGRAM_BOT_TOKEN=your_bot_token_here
TELEGRAM_CHAT_ID=your_chat_id_here

# PayPal Donation URL (Optional - only if implementing donations)
DONATION_URL=https://your-donation-url-here

# Application Settings
LOG_LEVEL=INFO
ENVIRONMENT=development
PORT=8000

